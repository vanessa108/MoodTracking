    android:background="@color/white">

    <ImageView
        android:id="@+id/activity_semicircle"
        android:layout_width="250dp"
        android:layout_height="180dp"
        android:layout_below="@+id/stats_30days"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        app:srcCompat="@drawable/activity_semicircle" />

    <ImageView
        android:id="@+id/sleep_semicircle"
        android:layout_width="250dp"
        android:layout_height="180dp"
        android:layout_below="@+id/activity_semicircle"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        app:srcCompat="@drawable/sleep_semicircle" />

    <Button
